{
  "manifest_version": 2,
  "name": "My Tab Manager",
  "version": "1.0",
  "description": "A custom tab management extension in the sidebar",
  "permissions": ["tabs", "storage"],
  "sidebar_action": {
    "default_panel": "sidebar.html",
    "default_title": "Tab Manager"
  },
  "background": {
    "scripts": ["js/background.js"],
    "type": "module",
    "persistent": false
  },
  "web_accessible_resources": [
    "js/tabsGroup.js",
    "editGroup.html",
    "js/editGroup.js"
  ]
}